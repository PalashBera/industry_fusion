<%= render layout: "shared/filter_modal", locals: { resource: @search, namespace: "master", modal_header: "Search Vendors" } do |f| %>
  <div class="form-row">
    <div class="form-group col-md-6 col-sm-12">
      <%= f.label :name_cont_any, "Name" %>
      <%= f.search_field :name_cont_any, placeholder: "Name", title: "Type vendor name", class: "form-control", autocomplete: :off %>
    </div>

    <div class="form-group col-md-6 col-sm-12">
      <%= f.label :email_cont_any, "Email" %>
      <%= f.search_field :email_cont_any, placeholder: "Email", title: "Type vendor email", class: "form-control", autocomplete: :off %>
    </div>
  </div>
<% end %>
